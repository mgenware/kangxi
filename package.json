{
  "name": "kangxi-editor",
  "version": "0.2.0",
  "description": "Another WYSIWYG editor",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/kangxi",
  "bugs": "https://github.com/mgenware/kangxi/issues",
  "keywords": [
    "layit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/kangxi.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "yarn run lint && yarn run clean && cross-env NODE_ENV=production rollup -c && node ./postbuild/postbuild",
    "compile": "tsc",
    "dev": "yarn run clean && cross-env NODE_ENV=development rollup -c -w",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "clean": "del-cli dist",
    "test": "yarn build"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "dependencies": {
    "prosemirror-commands": "^1.0.0",
    "prosemirror-dropcursor": "^1.0.0",
    "prosemirror-gapcursor": "^1.0.0",
    "prosemirror-history": "^1.0.0",
    "prosemirror-inputrules": "^1.0.0",
    "prosemirror-keymap": "^1.0.0",
    "prosemirror-model": "^1.0.0",
    "prosemirror-schema-list": "^1.0.0",
    "prosemirror-state": "^1.0.0",
    "prosemirror-view": "^1.0.0",
    "throw-if-arg-empty": "^2.1.0",
    "to-type-string": "^1.0.1"
  },
  "devDependencies": {
    "@types/prosemirror-commands": "^1.0.0",
