    "dev": "yarn run clean && cross-env NODE_ENV=development rollup -c -w",
    "lint": "eslint --max-warnings 0 --ext .ts src/",
    "clean": "del-cli dist",
    "test": "yarn build"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
